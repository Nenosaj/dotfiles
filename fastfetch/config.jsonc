{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "auto",
        "source": "/home/nenosaj/HDD/Pictures/makise3-removebg.png",
        "width": 20,
        "height": 10
    },
    "display": {
        "separator": "  ",
        "color": {
            "keys": "white"
        },
