        "os",
        "kernel",
        "wm",
        "terminal",
        "memory",
        "cpu",
        "packages",
        "uptime",
        {
            "type": "colors",
            "key": "Colors",
            "block": {
                "range": [1, 6]
            }
        }
    ]
}



